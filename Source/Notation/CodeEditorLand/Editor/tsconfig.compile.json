{
	"compilerOptions": {
		"outDir": "../../../../Target/CodeEditorLand/Editor",
		"rootDir": "../../../../Dependency/Land/Dependency/Editor/Source",
		"baseUrl": "../../../../Dependency/Dependency/Editor/Source",

		"jsx": "preserve",

		"emitDeclarationOnly": false,
		"noEmit": false,

		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,

		"lib": ["ES2022", "DOM", "DOM.Iterable", "WebWorker.ImportScripts"],

		"useDefineForClassFields": false,

		"noImplicitReturns": true,
		"noImplicitOverride": true,
		"noUnusedLocals": true,
		"allowUnreachableCode": false,
		"strict": true,
		"allowImportingTsExtensions": false,
		"isolatedModules": false,
		"exactOptionalPropertyTypes": false,

		"paths": {
			"vs/*": ["../../../../Dependency/Dependency/Editor/Source/vs/*"]
		},

		"module": "ESNext",
		"moduleResolution": "Bundler",

		"target": "ESNext",

		"verbatimModuleSyntax": false
	},
	"exclude": ["**/node_modules/**", "**/Target/**"],
	"extends": "@playform/build/tsconfig",
	"include": [
		"../../../../Dependency/Dependency/Editor/Source/*.ts",
		"../../../../Dependency/Dependency/Editor/Source/typings",
		"../../../../Dependency/Dependency/Editor/Source/vs/**/*.ts",
		"../../../../Dependency/Dependency/Editor/Source/vscode-dts/vscode.proposed.*.d.ts",
		"../../../../Dependency/Dependency/Editor/Source/vscode-dts/vscode.d.ts"
	],
	"watchOptions": {
		"excludeDirectories": ["**/node_modules/**", "**/Target/**"]
	}
}
