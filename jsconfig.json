{
	"$schema": "https://json.schemastore.org/jsconfig",
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"target": "ESNext",
		"jsx": "preserve",
		"esversion": 6,
		"strictNullChecks": true,
		"strictFunctionTypes": true
	},
	"exclude": ["node_modules"],
	"jshint.options": {
		"esversion": 6
	},
	"extends": "@playform/build/jsconfig"
}
