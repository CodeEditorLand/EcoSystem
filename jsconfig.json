{
	"$schema": "https://json.schemastore.org/jsconfig",
	"compilerOptions": {
		"baseUrl": "./",
		"emitDeclarationOnly": false,
		"noEmit": true,
		"outDir": "Target",
		"rootDir": "."
	},
	"exclude": ["**/node_modules/**", "**/Target/**"],
	"extends": "@playform/build/jsconfig",
	"include": ["./Dependency/", "./Elements/", "./Vanilla/"],
	"watchOptions": {
		"excludeDirectories": ["**/node_modules/**", "**/Target/**"]
	}
}
