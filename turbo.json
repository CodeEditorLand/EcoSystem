{
	"$schema": "https://turbo.build/schema.json",
	"envMode": "loose",
	"extends": ["//"],
	"tasks": {
		"Run": {
			"cache": false,
			"dependsOn": ["^prepublishOnly"],
			"persistent": true
		},
		"prepublishOnly": {
			"dependsOn": ["^prepublishOnly"],
			"outputs": ["**/Target/**"],
			"passThroughEnv": ["TAURI"]
		}
	},
	"ui": "stream"
}
