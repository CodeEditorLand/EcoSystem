{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"Run": {
			"cache": false,
			"dependsOn": ["^prepublishOnly", "^Run"],
			"persistent": true
		},
		"prepublishOnly": {
			"dependsOn": ["^prepublishOnly"],
			"outputs": ["Target/**"]
		},
		"tauri": {
			"dependsOn": ["^tauri"],
			"outputs": ["Target/**"]
		}
	},
	"ui": "stream",
	"extends": ["//"]
}
